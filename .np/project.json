{
	"projectname": "cpp_playground",
	"cstdversion": "gnu23",
	"cppstdversion": "gnu++23",
	"threads": 4,
	"configurations": [
		{
			"configname": "cpp_playground_la64",
			"os": "linux",
			"arch": "arm64",
			"gcc": "gcc",
			"g++": "g++",
			"bindir": "bin/la64",
			"objdir": "obj/la64",
			"gccargs": [
				"-Wall",
				"-Wextra",
				"-pedantic",
				"-pg",
				"-g3",
				"-Os",
				"-flto",
				"-save-temps",
				"-fdiagnostics-color=always"
			],
			"gppargs": [
				"-Wall",
				"-Wextra",
				"-pedantic",
				"-pg",
				"-g3",
				"-Os",
				"-flto",
				"-save-temps",
				"-fdiagnostics-color=always"
			],
			"sourcefiles": [
				"src/main.cpp"
			],
			"cincludedirs": [
				"/usr/local/include/c++/13.1.1/"
			],
			"cppincludedirs": [
				"/usr/local/include/c++/13.1.1/"
			],
			"libdirs": [
